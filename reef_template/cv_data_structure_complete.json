    "metadata": {
      "extraction_notes": [
        "All fields are nullable - can be null if not found in CV",
        "Empty strings are converted to null during processing",
        "Empty arrays remain as empty arrays",
        "Preserve original language and Unicode characters",
        "No translation or normalization of text",
        "Exact text preservation for responsibilities and achievements",
        "Claude 4 Opus used with temperature 0.0 for deterministic extraction"
      ],
      
      "supported_date_formats": [
        "yyyy-mm",
        "mm/yyyy",
        "month year",
        "year only",
        "Present/Current",
        "Range formats (e.g., '2020 - 2023')"
      ],
      
      "skills_exclusions": [
        "Languages (go to languages section)",
        "Soft skills like 'Team Player', 'Communication'",
        "Hobbies (go to hobbies section)",
        "Certifications (go to certifications section)",
        "Generic terms like 'Computer Skills'"
      ],
      
