    "motion",
    "clsx",
    "tailwind-merge",
    "three",
    "three-globe",
    "@react-three/fiber@alpha",
    "@react-three/drei"
  ],
  "files": [
    {
      "name": "globe-base.tsx",
      "content": "components/ui/globe/globe-base.tsx"
