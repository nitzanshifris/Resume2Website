    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "canvas-confetti": "^1.9.2",
    "fast-glob": "^3.3.2",
    "jsdom": "^26.1.0",
    "react-docgen-typescript": "^2.4.0",
