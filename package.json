{
  "name": "cv2web-monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "packages/*",
    "aceternity-components-library",
    "aceternity-gallery-app"
  ],
  "scripts": {
    "dev:renderer": "pnpm --filter new-renderer dev",
    "build:renderer": "pnpm --filter new-renderer build",
    "lint:renderer": "pnpm --filter new-renderer lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "gen:stories": "node scripts/generate-stories.js"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/nextjs": "8.6.14",
    "@storybook/react": "8.6.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "jsdom": "^26.1.0",
    "storybook": "8.6.14",
    "vite": "^7.0.2",
    "vitest": "^3.2.4",
    "@cv2web/design-tokens": "workspace:*",
    "@aceternity/components-library": "workspace:*",
    "fast-glob": "^3.3.2"
  }
}